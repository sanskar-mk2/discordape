---
export interface Props {
    title: string;
}
const { title } = Astro.props.frontmatter;
import Wrapper from "./Wrapper.astro";
---

<Wrapper>
    <div class="w-full flex flex-col items-center">
        <section class="container my-4 bg-base-200 rounded bg-opacity-50">
            <article class="prose p-1 sm:p-2 md:p-4 lg:p-8">
                <h1>{title}</h1>
                <slot />
            </article>
        </section>
    </div>
</Wrapper>
